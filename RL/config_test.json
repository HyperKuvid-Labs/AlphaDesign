{
  "max_generations": 3,
  "population_size": 3,
  "neural_network_enabled": true,
  "save_frequency": 1,
  "max_runtime_hours": 1,
  
  "early_stopping": {
    "patience": 10,
    "min_delta": 0.005,
    "monitor": "fitness",
    "stagnation_threshold": 10,
    "convergence_threshold": 0.001
  },
  
  "neural_network": {
    "learning_rate": 0.001,
    "batch_size": 8,
    "training_frequency": 10,
    "epochs_per_training": 10,
    "save_best_model": true
  },
  
  "genetic_algorithm": {
    "crossover_rate": 0.85,
    "mutation_rate": 0.80,
    "elite_ratio": 0.15,
    "tournament_size": 3
  },
  
  "cfd_analysis": {
    "enabled": true,
    "parallel_processes": 1,
    "timeout_seconds": 60,
    "smart_skipping": true,
    "save_results": true,
    "results_output_dir": "cfd_results"
  },
  
  "output": {
    "save_all_stl": false,
    "save_best_only": true,
    "generate_reports": true,
    "create_visualizations": false
  }
}
